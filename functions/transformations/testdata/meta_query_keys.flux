from(bucket:"test")
    |> range(start:2018-05-22T19:53:26Z)
    |> filter(fn: (r) => r._measurement == "cpu")
    |> keys() 
    |> yield(name:"0")

from(bucket:"test")
    |> range(start:2018-05-22T19:53:26Z)
    |> filter(fn: (r) => r._measurement == "cpu")
    |> group(columns: ["host"])
    |> distinct(column: "host")
    |> group()
    |> yield(name:"1")